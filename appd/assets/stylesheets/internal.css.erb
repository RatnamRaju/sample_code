body{
	font-size:15px;
	line-height:19px;
	font-family:'MyriadWebCondensed', arial, sans-serif;
	color:#333;
	background-color:#f5f4fb;
	text-align:center;
	width:100%;
	height:100%;
}

a:link, 
a:visited, 
a:active{ color:#ba3b11 /* red color */;text-decoration:none; }
a:hover,a:focus{ color:#000;text-decoration:underline; }
a{ outline:none; }

h1, h2, h3 { background-color:transparent;font-weight:normal; }
h1 { font-size:26px;line-height:30px;color:#545454;font-family:'HelveticaNeueLTCom65Medium'; }
h2 { font-size:20px;line-height:24px;color:#ba3b11;font-family:'MyriadBold';letter-spacing:-0.2px; }
	
p { letter-spacing:0.1px; }

.bold { font-family:'MyriadBold'; }
.semibold { font-family:'MyriadProSemibold'; }
.regular { font-family: 'MyriadWebRegular'; }


.clear{ clear:both;height:1px;font-size:1px;border:none;background:transparent; }
a img{ border:none }
form fieldset{ border:none; }
form legend{ display:none; }


/*-- STRUCTURE --*/

/* 100% width divs because of the background images */

.wrapper { position:relative;width:100%;overflow:hidden;text-align:left;margin:0;padding:0;background:#fff url(<%= asset_path 'body-bg.png' %>) repeat-x 0 0;  }

.header { position:relative;width:100%;height:122px;margin:0;background:url(<%= asset_path 'logo-back-shadow.png' %>) no-repeat 50% 0; z-index: 10001;}
.header.home { background:url(<%= asset_path 'logo-back-shadow-home.png' %>) no-repeat 50% 0; }

.content { position:relative;overflow:hidden;width:100%;margin:0;padding:0 0 50px 0;background:url(<%= asset_path 'content-bg.png' %>) no-repeat 50% 0; } /*-- Steps pages --*/
.content.none { position:relative;width:100%;overflow:hidden;margin:0;padding:0 0 50px 0;background:#fff; } /*-- NO BACKGROUND --*/

.content.home { margin-top:-18px;padding:0;background:url(<%= asset_path 'hotspot.png' %>) no-repeat 50% 18px; } /*-- Homepage --*/
.content.home.big { margin-top:-18px;padding:0;background:url(<%= asset_path 'hotspot-big.png' %>) no-repeat 50% 18px; } /*-- Homepage big hotspot --*/
.content.wide { padding-bottom:35px;background:url(<%= asset_path 'content-wide-bg.png' %>) no-repeat 50% 0; } /*-- Privacy and terms type pages --*/
.content.btm { padding-bottom:40px;height:84px;background:url(<%= asset_path 'content-btm.png' %>) no-repeat 50% 0; } /*-- Privacy and terms type pages --*/
.content.free7day { margin-top:-18px;background:url(<%= asset_path 'hotspot-smaller.png' %>) no-repeat 50% 18px; }
.content.pricing { padding-bottom:27px;background:url(<%= asset_path 'pricing-content-bg.png' %>) no-repeat 50% 0; } /*-- Pricing page content background --*/

.content.hot-squeeze { padding-bottom:0;background:url(<%= asset_path 'hotspot-squeeze.png' %>) no-repeat 50% 0; } /*-- Squeeze hotspot --*/
.content.squeeze { padding-bottom:35px;background:url(<%= asset_path 'content-wide-squeeze.png' %>) no-repeat 50% 0; } /*-- Content wide with white background --*/
.content.squeeze-top { min-height:700px;padding-bottom:55px;background:url(<%= asset_path 'squeeze-top-shadow.png' %>) no-repeat 50% 0; } /*-- Squeeze top two columns with shadow --*/

.content.shadow { padding:29px 0 0 0;background:url(<%= asset_path 'shadow-1050px.jpg' %>) no-repeat 50% 0; } /*-- Content shadow --*/

.content.generate-code { height:482px;padding:10px 0 50px 0;background:url(<%= asset_path 'generate-code-largebg.png' %>) no-repeat 50% 0; } /*-- Generate code stripes background with shadow --*/

.content.marketing-tools { min-height:235px;margin-top:10px;padding:40px 0 50px 0;background:url(<%= asset_path 'shadow-big-tools.png' %>) no-repeat 50% 0; }

.center { position:relative;width:990px;margin:0 auto; } /* This div centers content where needed */

/* iPad [portrait + landscape] */
@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {
 .center { margin: 10px; }
}
/* iPhone [portrait + landscape] */
@media only screen and (max-device-width: 480px) {
 .center { margin: 10px; }
}

.footer { position:relative;width:100%;min-height:139px;background:#f5f4fb url(<%= asset_path 'footer-bg.png' %>) repeat-x 0 0;text-align:left; }





/*-- HEADER --*/


h1.logo { float:left;width:261px;height:63px;padding:30px 0 0 0; }
h1.logo a { display:block;text-indent:-9000px;width:261px;height:63px;background:url(<%= asset_path 'logo.png' %>) no-repeat 0 0; }

div.header-right { position:relative;float:right;margin:0;padding:30px 0 0 0;text-align:right }

	div.header-right h1 { line-height:36px; }
	div.header-right h2 { line-height:40px; }
	
div.login { width:90px;position:relative;float:right;overflow:hidden;height:85px;margin:25px 0 0 25px;background:url(<%= asset_path 'login-shadow.png' %>) no-repeat 0 0; }
	p.log-btn { display:block;float:right;padding:32px 0 0 0;line-height:15px;font-size:15px;font-family:'MyriadProSemibold'; }
	p.log-btn a { color:#ba3b11;padding-right:20px;background:url(<%= asset_path 'nav-login.png' %>) no-repeat right 2px; }

ul.nav { position:relative;list-style:none;overflow:hidden }
	ul.nav li { float:right;display:inline;border-right:1px solid #a5a5a5;padding:0 18px;line-height:15px;font-size:15px;font-family:'MyriadProSemibold';color:#545454 }
	ul.nav.condensed li { font-family:'MyriadWebCondensed'; }
	ul.nav li.last { border:0;padding-right:0 }
	ul.nav li#login a { color:#ba3b11;padding-right:20px;background:url(<%= asset_path 'nav-login.png' %>) no-repeat right 2px; }
	ul.nav li#contact a { color:#ba3b11;padding-right:20px;background:url(<%= asset_path 'nav-contact.png' %>) no-repeat right 2px; }
	ul.nav li#help a { color:#ba3b11;padding-right:20px;background:url(<%= asset_path 'nav-help.png' %>) no-repeat right 2px; }
	ul.nav li#settings a { color:#ba3b11;padding-right:20px;background:url(<%= asset_path 'nav-settings.png' %>) no-repeat right 2px; }
	ul.nav li span { color:#ba3b11;padding-left:5px; }
	ul.nav li a { color:#545454 }
	ul.nav li a:hover, ul.nav li a.active { color:#ba3b11 }
	ul.nav.top li#login a.active, ul.nav li#contact a.active { color:#545454 }
	ul.nav.condensed li a { color:#ba3b11 }

	
/*-- #END header --*/







/*-- HOMEPAGE --*/

