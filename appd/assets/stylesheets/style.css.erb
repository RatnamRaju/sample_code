body {
	font-size:17px;
	line-height:19px;
	font-family:'MyriadWebCondensed', arial, sans-serif;
	color:#333;
	background-color:#f5f4fb;
	text-align:center;
	width:100%;
	height:100%;
}

a:link,a:visited,a:active {
	color:#ba3b11;
	text-decoration:none;
}

a:hover,a:focus {
	color:#000;
	text-decoration:underline;
}

a {
	outline:none;
}

h1,h2,h3 {
	background-color:transparent;
	font-weight:400;
}

h1 {
	font-size:26px;
	line-height:30px;
	color:#545454;
	font-family:'HelveticaNeueLTCom65Medium';
}

h2 {
	font-size:20px;
	line-height:24px;
	color:#ba3b11;
	font-family:'MyriadBold';
	letter-spacing:-0.2px;
}

p {
	letter-spacing:0.1px;
}

.bold {
	font-family:'MyriadBold';
}

.semibold {
	font-family:'MyriadProSemibold';
}

.regular {
	font-family:'MyriadWebRegular';
}

.small {
  color: #3BBA11;
  font-size: 10px;
}

.clear {
	clear:both;
	height:1px;
	font-size:1px;
	border:none;
	background:transparent;
}

.wrapper {
	position:relative;
	width:100%;
	min-width: 1024px;
	overflow:hidden;
	text-align:left;
	background:#fff url(<%= asset_path 'body-bg.png' %>) repeat-x 0 0;
	margin:0;
	padding:0;
}


