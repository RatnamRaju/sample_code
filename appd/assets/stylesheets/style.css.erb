	height:122px;
	background:url(<%= asset_path 'logo-back-shadow.png' %>) no-repeat 50% 0;
	margin:0;
}

.header.home {
	background:url(<%= asset_path 'logo-back-shadow-home.png' %>) no-repeat 50% 0;
}

.content {
	position:relative;
	width:100%;
	overflow:hidden;
	background:url(<%= asset_path 'content-bg.png' %>) no-repeat 50% 0;
	margin:0;
	padding:0 0 50px;
}

.content.home {
	margin-top:-18px;
	background:url(<%= asset_path 'hotspot.png' %>) no-repeat 50% 18px;
	padding:0;
}

.content.home.big {
	margin-top:-18px;
	padding:0;
	background:url(<%= asset_path 'hotspot-big.png' %>) no-repeat 50% 18px; 
}

